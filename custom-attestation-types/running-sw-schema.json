{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "fingerprint": {
        "type": "string",
        "pattern": "^[a-f0-9]{64}$"
      },
      "flow_name": {
        "type": "string"
      },
      "git_commit": {
        "type": "string",
        "pattern": "^(|[a-f0-9]{40})$"
      },
      "name": {
        "type": "string"
      },
      "template_reference_name": {
        "type": "string"
      }
    },
    "required": ["fingerprint", "flow_name", "git_commit", "name", "template_reference_name"]
  }
}
